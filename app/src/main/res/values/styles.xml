<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="number_style">
        <item name="android:textSize">25sp</item>
        <item name="android:background">@drawable/round_number_bg</item>
    </style>

    <style name="fuhao_style">
        <item name="android:background">@drawable/round_corner_bg</item>
        <item name="android:textSize">25sp</item>
        <item name="android:textColor">#016496</item>
    </style>

    <style name="deng_style">
        <item name="android:background">@drawable/round_deng_bg</item>
        <item name="android:textSize">25sp</item>
        <item name="android:textColor">#fff</item>
    </style>

</resources>
